<!DOCTYPE html>

<html>
<head>

    <title>@ViewBag.Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- plugins:css -->

    <link rel="stylesheet" href="~/vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/vendors/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link href="~/css/style.css" rel="stylesheet" />
    <!-- endinject -->

    <link rel="shortcut icon" href="~/img/favicon1.png" />
</head>

<body>

    <div class="container-scroller">
        
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">

@*                <a class="navbar-brand brand-logo me-5" href="index.html"><img src="~/img/faces/logo.svg" class="me-2" alt="logo" /></a>
*@                <b><h2>SMS</h2></b>
@*                <a class="navbar-brand brand-logo-mini" href="index.html"><img src="" alt="logo" /></a>
*@            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
                
                <ul class="navbar-nav navbar-nav-right">
                   
                    
                <li class="nav-item nav-profile dropdown">
                    <img src="~/img/faces/face28.jpg" alt="profile" />
                    <a  asp-action="Index1" asp-controller="Home" id="profileDropdown">
                        Logout
                    </a>
                    
                </li>
                </ul>
                
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    
                    <li class="nav-item">
                        <a class="nav-link" asp-action="getstudentdetails" asp-controller="admin">
                            <i class="ti-view-list-alt menu-icon"></i>
                            <span class="menu-title">Student Details</span>
                        </a>
                    </li>
                    


                    <li class="nav-item">
                        <a class="nav-link" asp-action="getcourselist" asp-controller="admin">
                            <i class="ti-pie-chart menu-icon"></i>
                            <span class="menu-title">Student Enrollment</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="todaysenrollment" asp-controller="enrollment">
                            <i class="ti-star menu-icon"></i>
                            <span class="menu-title">Todays Course Enrollment</span>
                        </a>

                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="course">
                            <i class="ti-star menu-icon"></i>
                            <span class="menu-title"> Courses Details</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="viewcollege" asp-controller="college">
                            <i class="ti-star menu-icon"></i>
                            <span class="menu-title"> Colleges Details</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="viewfeedback" asp-controller="feedback">
                            <i class="ti-star menu-icon"></i>
                            <span class="menu-title"> View Student Feedback</span>
                        </a>
                    </li>
                    
                </ul>
            </nav>
            <!-- partial -->

            @RenderBody()



            <!-- main-panel ends -->
        </div>
        <footer id="footer">
            <div class="container">
                <div class="d-sm-flex justify-content-center justify-content-sm-between">
                    <div class="copyright" style="text-align-last:auto">
                        &copy; Copyright <strong><span>Student Management System</span></strong>. All Rights Reserved
                    </div>
                    
                    
                </div>
                </div>
        </footer>
        <!-- page-body-wrapper ends -->
    </div>

    <!-- plugins:js -->

    <script src="~/vendor/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->


    <script src="~/vendors/chart.js/Chart.min.js"></script>

    <script src="~/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/js/off-canvas.js"></script>
    <script src="~/js/hoverable-collapse.js"></script>
    <script src="~/js/template.js"></script>
    <script src="~/js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/js/dashboard.js"></script>
    <!-- End custom js for this page-->
    <!-- partial:partials/_footer.html -->

</body>
</html>
